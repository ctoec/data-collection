    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.24",
    "@types/underscore": "^1.10.24",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.1",
    "class-validator": "^0.12.2",
    "concurrently": "^6.0.2",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "faker": "^5.1.0",
    "http-proxy": "^1.18.1",
    "https": "^1.0.0",
    "idx": "^2.5.6",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.12.1",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "mssql": "^6.3.1",
    "multer": "^1.4.2",
    "nodemon": "^2.0.7",
    "reflect-metadata": "^0.1.10",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.10.2",
    "tsoa": "^3.2.1",
    "typeorm": "^0.2.29",
    "typescript": "^3.9.7",
    "underscore": "^1.12.1",
    "uuid": "^8.3.0",
    "xlsx": "^0.16.4",
    "xmldom": "0.5.0"
  },
  "resolutions": {
    "mssql/**/axios": "0.21.1",
    "mssql/**/xmldom": "0.5.0",
    "mssql/**/underscore": "1.12.1",
    "jest/**/lodash": "^4.17.21",
    "tsoa/@tsoa/cli/handlebars": "^4.7.7",
    "tsoa/@tsoa/cli/merge": "^2.1.1",
    "jest/**/hosted-git-info": "^3.0.8",
    "concurrently/**/hosted-git-info": "^3.0.8"
  },
  "scripts": {
    "build": "rm -rf dist/ && npm run build-ts",
    "build-ts": "tsc",
    "debug": "npm run build && npm run watch-debug",
    "local": "NODE_ENV=development MULTI_ORG_USER=false PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true yarn run watch",
    "prettier-all": "prettier --single-quote --write **/*.{js,jsx,ts,tsx,json,css,scss}",
    "prettier": "prettier --single-quote --write",
    "serve": "node dist/src/index.js",
    "serve-debug": "nodemon --inspect dist/src/index.js",
    "start": "npm run serve",
    "test": "jest --runInBand",
    "e2e": "yarn --cwd e2e-tests test",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js ",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/src/index.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/puppeteer": "^5.4.0",
    "command-line-args": "^5.1.1",
    "husky": "^4.3.0",
    "prettier": "^2.0.5",
    "puppeteer": "^5.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/pre-commit.sh"
    }
  }
}
